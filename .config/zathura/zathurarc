map f scroll full-down
map b scroll full-up

map . navigate next
map , navigate previous

#unmap <PageDown>
#unmap <PageUp>
#map <Return> scroll half-down
#map . scroll half-up

map <C-s> exec pdfimages -p -f $PAGE -l $PAGE -all "$FILE" images/

#map <C-f> follow

set guioptions 'svh'
set scroll-step 200
set scroll-full-overlap 0.15
set show-hidden true
